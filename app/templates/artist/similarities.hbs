<div class="page-header">
  <h1>{{name}}</h1>
</div>

<h2>Similar Artists</h2>

<div class="row">
<div class="col-lg-12">

  {{#each model}}
    <form class="form-inline" role="form">
      <div class="form-group">
        <div class="input-group">
          {{input value=other_artist class='form-control'}}
        </div>
      </div>
      <div class="form-group">
        <div class="input-group">
        {{view Ember.Select content=weights value=weight class='form-control'}}
        </div>
      </div>
    </div>
  {{/each}}

</div>
